{
  "name": "{{ name }}",
  "version": "{{version}}",
  "description": "{{ description }}",
  "author": "{{ author }}",
  "scripts": {

  },
  "dependencies": {
    "vue": "^2.0.0",
    {{#each suit}}
    {{this.name}}:{{this.version}}
    {{/each}}
  },
  "devDependencies": {
      "babel-core": "^6.1.2",
      "babel-loader": "^6.1.0",
      "babel-plugin-transform-runtime": "^6.1.2",
      "babel-preset-es2015": "^6.1.2",
      "babel-preset-stage-0": "^6.1.2",
      "babel-runtime": "^5.8.0",
      "css-loader": "^0.25.0",
      "less": "^2.5.3",
      "less-loader": "^2.2.1",
      "vue-loader": "^9.7.0",
      "vue-style-loader": "^1.0.0",
      {{#lint}}
      "babel-eslint": "^7.0.0",
      {{/lint}}
      {{#if webpack2}}
      "webpack": "^2.2.0-rc.4",
      {{else}}
      "webpack": "^1.12.2",
      {{/if}}
      "webpack-dev-server": "^1.12.0",
      {{#unit}}
      "mocha": "^3.1.0",
      "chai": "^3.5.0",
      {{/unit}}
  },
  "engines": {
    "node": ">= 5.0.0",
